<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/LinearVerticalDefault">
    <com.anangkur.footbalku.base.BaseErrorView
        android:id="@+id/error_match"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:gravity="center"/>
    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/swipe_match"
        style="@style/SwipeRefreshDefault">
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recycler_match"
            style="@style/RecyclerDefault"/>
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
</LinearLayout>